{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-zlib": "*",
        "althinect/filament-spatie-roles-permissions": "^2.2.21",
        "awcodes/filament-quick-create": "^3.3",
        "awcodes/filament-table-repeater": "^2.1.1",
        "awcodes/filament-tiptap-editor": "^3.2.23",
        "cms-multi/filament-clear-cache": "^2.0.4",
        "coolsam/flatpickr": "^3.0.1",
        "cybercog/laravel-optimus": "^3.10",
        "doctrine/dbal": "^3.8.2",
        "filament/actions": "^3.0-stable",
        "filament/filament": "^3.0-stable",
        "filament/forms": "^3.0-stable",
        "filament/infolists": "^3.0-stable",
        "filament/notifications": "^3.0-stable",
        "filament/spatie-laravel-media-library-plugin": "^3.0-stable",
        "filament/spatie-laravel-tags-plugin": "^3.0-stable",
        "filament/tables": "^3.0-stable",
        "filament/widgets": "^3.0-stable",
        "flowframe/laravel-trend": "^0.1.5",
        "guzzlehttp/guzzle": "^7.8.1",
        "hammerstone/fast-paginate": "^1.0.1",
        "jaocero/radio-deck": "^1.2.2",
        "jenssegers/date": "^4.0",
        "juliomotol/laravel-auth-timeout": "^4.0.1",
        "kenepa/multi-widget": "^2.0.2",
        "kenepa/resource-lock": "^2.0.6",
        "lara-zeus/matrix-choice": "^3.2",
        "laravel/framework": "^10.44.0",
        "laravel/octane": "^2.3.3",
        "laravel/sanctum": "^3.3.3",
        "laravel/tinker": "^2.9.0",
        "leandrocfe/filament-apex-charts": "^3.1.2",
        "livewire/livewire": "^3.4.4",
        "maatwebsite/excel": "^3.1.53",
        "mallardduck/blade-lucide-icons": "^1.11",
        "malzariey/filament-daterangepicker-filter": "^2.3.1",
        "marjose123/filament-lockscreen": "^2.0.3",
        "moox/jobs": "^2.0.9",
        "phpsa/filament-password-reveal": "^2.1.1",
        "predis/predis": "^2.2.2",
        "propaganistas/laravel-phone": "^5.1.1",
        "psr/simple-cache": "^3.0",
        "saade/filament-autograph": "^3.0.2",
        "shuvroroy/filament-spatie-laravel-backup": "^2.1.3",
        "spatie/eloquent-sortable": "^4.1.1",
        "spatie/laravel-google-cloud-storage": "^2.2.6",
        "spatie/laravel-pdf": "^1.2",
        "tapp/filament-auditing": "^3.0.4",
        "tapp/filament-authentication-log": "^3.0.2",
        "tavo1987/ec-validador-cedula-ruc": "^1.0.2",
        "torann/geoip": "^3.0.5",
        "wildside/userstamps": "^2.3",
        "wnx/laravel-backup-restore": "^1.3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10.5",
        "barryvdh/laravel-ide-helper": ">=2.15.1",
        "fakerphp/faker": "^1.23.1",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.13.11",
        "laravel/sail": "^1.27.4",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "^7.10",
        "phpstan/phpstan": "^1.10.58",
        "phpunit/phpunit": "^10.5.10",
        "spatie/laravel-ignition": "^2.4.2",
        "tightenco/duster": "^2.7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
